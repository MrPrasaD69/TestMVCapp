@model List<PrasadApp.Context.Table_1>
@{
    ViewBag.Title = "StudentList";
}

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Address</th>
        <th>Action</th>
    </tr>

    @foreach(var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Phone</td>
                <td>@item.Address</td>
                <td>
                    <a href="@Url.Action("Student",new { item.Name, item.Email, item.Phone, item.Address})" class="btn btn-success">Edit</a>
                    <a href="@Url.Action("Delete",new { item.Id})" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
</table>
